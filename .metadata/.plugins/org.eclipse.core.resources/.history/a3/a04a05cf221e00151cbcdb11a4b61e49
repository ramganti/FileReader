  <adfilesmodel:FileAdapterConfiguration name="FileAdapter" description="testone" instanceId="testone" reporting="#/5" logging="#/6" advisories="#/7 #/8 #/9 #/10 #/11" monitoring="#/12" administration="#/13" transports="#/1 #/2 #/3 #/14" channels="#/4">
